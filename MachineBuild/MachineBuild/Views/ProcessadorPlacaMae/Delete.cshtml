@model MachineBuild.Models.Pecas.ProcessadorPlacaMae

@{
    ViewBag.Title = "Excluir";
}

<h2>#Excluir</h2>

<h3>Você tem certeza que deseja excluir este item?</h3>
<div>
    <h4>Compatibilidade</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayName("Placa Mãe")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.PlacaMae.Nome)
        </dd>

        <dt>
            @Html.DisplayName("Processador")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Processador.Nome)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Excluir" class="btn btn-default" /> |
            @Html.ActionLink("Voltar", "Index")
        </div>
    }
</div>
